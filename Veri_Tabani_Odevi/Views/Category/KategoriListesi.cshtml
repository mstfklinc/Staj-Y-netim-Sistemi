@model IEnumerable<Veri_Tabani_Odevi.Models.Category>

<div class="well">
    <h4>Staj Kategorileri</h4>
    <div class="row">

        @foreach (var item in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li>
                        <a href="/Staj/List/@item.CategoryId">@item.KategoriAdi</a>
                    </li>
                </ul>
            </div>
        }

        
    </div>

</div>
